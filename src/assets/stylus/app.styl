@keyframes lds-dual-ring {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}
#nprogress {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 999999;
  background: #ccc9;
  .bar {
    height: 2px;
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    background:#3c8dbc
  }
  .spinner {
    display: inline-block;
    width: 64px;
    height: 64px;
    position: absolute;
    transform: translate(-50%, -50%);
    top: 50%;
    left: 50%;
    background-image: url(/static/logo_01.png);
    background-size: 70%;
    background-position: center;
    background-repeat: no-repeat
  }
  .spinner:after {
    content: " ";
    display: block;
    width: 64px;
    height: 64px;
    margin: 1px;
    border-radius: 50%;
    border: 5px solid #fff;
    border-color: #fff transparent #fff transparent;
    animation: lds-dual-ring 1.2s linear infinite;
  }
}
